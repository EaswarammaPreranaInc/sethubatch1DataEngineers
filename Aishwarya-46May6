#Find outputs
from threading import *
def disp():
	for i in range(10):
		print('child thread')
child=Thread(target=disp)
child.start()
child.join()
for i in range(10):
	print('main thread')

#Output (executed sequentially due to join)
child thread #10 times
main thread #10 times 



#Find outputs
from threading import *
import time
def disp():
	for i in range(10):
		print('child thread')
		time.sleep(2)
child=Thread(target=disp)
child.start()
child.join(10)
for i in range(10):
	print('main thread')

#output
child thread (5 times compulsory)
main thread (10 times continously)
child thread (remaining no of times)
