#program 6
# Find  outputs 

import   time
list=[('A' , 10) , ('B' , 20) , ('C' , 15) , ('D' , 5) , ('E' , 18)]
f = filter(lambda   x  :   x[1]  >=  12 , list) #  Creates  an  empty  filter  object
while   True:
	try:
		print(next(f))  #   ('B' , 20)  <next  line>  ('C' , 15)    <next  line>  ('E' , 18)  <next  line>
		time . sleep(1)
	except:
		break

#output
('B', 20)
('C', 15)
('E', 18)

