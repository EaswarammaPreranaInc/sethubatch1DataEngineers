#program 1
#program to month with  else  and  if
try:
	month=int(input("enter month number from (1-12) : "))
	if month==1:
		print("January")
	else:
		if month==2:
			print("February")
		else:
			if month==3:
				print("March")
			else:
				if month==4:
					print("April")
				else:
					if month==5:
						print("May")
					else:
						if month==6:
							print("June")
						else:
							if month==7:
								print("July")
							else:
								if month==8:
									print("Auguest")
								else:
									if month==9:
										print("September")
									else:
										if month==10:
											print("octobure")
										else:
											if month==11:
												print("november")
											else:
												if month==12:
													print("December")
												else:
													print("Invalid month number")
except:
	print("Input should be integer number")

 output:
 enter month number from (1-12) :12
 December
 

