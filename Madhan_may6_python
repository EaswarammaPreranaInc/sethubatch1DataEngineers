#2.Find  outputs (Home  work)

'''
from  threading  import *
import  time
def   disp():
	for  i  in  range(10):
		print('child  thread')
		time . sleep(2)
child = Thread(target = disp)
child . start() #child thread 5 times 
child . join(10) #main thread 10 times #child thread 5 times 
for  i  in  range(10):
	print('main  thread')
	
'''
