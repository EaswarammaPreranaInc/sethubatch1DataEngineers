#program 2
# Find  outputs  
import  sys
class  Test:
	def  __init__(self):
		print('Constructor  :  ' , id(self))
		return    None
	def  __del__(self):
		print('Destructor  :  ' , id(self))
		return  25
# End  of  the  class
t = Test()
print(t . __init__())
print(sys . getrefcount(t))
print(t . __del__())
print(sys . getrefcount(t))
print('Bye')

#output
Constructor  :   1845444307536
Constructor  :   1845444307536
None
2
Destructor  :   1845444307536
25
2
Bye
Destructor  :   1845444307536

