#program 4
# Find  outputs

class  c1:
	def  __init__(self):
		print('Object  is    created')
	def  __del__(self):
		print('Object  is  lost')
#End  of  the  class
def    f1():
        print('Function  begin')
        a  =  c1()
        print('Function  end')
        return   a
print('Program  Begin')
f1()
print('Program  End')

#output
Program  Begin
Function  begin
Object  is    created
Function  end
Object  is  lost
Program  End

